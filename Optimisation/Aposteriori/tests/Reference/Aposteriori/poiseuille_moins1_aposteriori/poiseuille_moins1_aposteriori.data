# PARALLEL NOT #
dimension 2

Pb_hydraulique_Aposteriori pb
Domaine dom
# BEGIN MESH #
Mailler dom
{
        Pave Entree
                {
                Origine 0. 0.
                Nombre_de_Noeuds 11 6
                Longueurs 1 0.5
                }
                {
                bord Entree X = 0.   0. <= Y <= 0.5
                bord Haut   Y = 0.5  0. <= X <= 1.0
                bord Bas    Y = 0.   0. <= X <= 1.0
                bord Sortie X = 1.0  0. <= Y <= 0.5
                }
}
trianguler dom
# END MESH #

# BEGIN PARTITION
Partition dom
{
  Partition_tool Tranche { tranches 2 1 }
  Larg_joint 2
  zones_name dom
}
End
END PARTITION #
# BEGIN SCATTER
Scatter dom.Zones dom
END SCATTER #

VEFprep1b dis

Schema_Euler_explicite sch
Read sch
{
	tinit 0
	tmax 5.
	dt_min 0.00001
	dt_max 1.0
	dt_impr 0.1
	nb_pas_dt_max 20
	dt_sauv 100
	seuil_statio 1.e-8
}


Champ_Uniforme gravite
Read gravite 2 0 0
Associate pb dom
Associate pb sch
Discretize pb dis
Read pb
{
   
   fluide_incompressible {
    	mu Champ_Uniforme 1 1.
   	rho Champ_Uniforme 1 1.
   }
   
   	Navier_Stokes_Aposteriori
	{
		solveur_pression GCP { 
			precond ssor { omega 1.650000 } 
			seuil 1.000000e-16 
			impr 
		}
		convection { amont }
		diffusion {  }
		conditions_initiales {
			vitesse Champ_Uniforme 2 0. 0.
			}
		boundary_conditions {
			Haut paroi_fixe
			Bas symetrie
			Entree frontiere_ouverte_pression_imposee Champ_Front_Uniforme 1 1.
			Sortie frontiere_ouverte_pression_imposee Champ_Front_Uniforme 1 0.
		}
	}
	Postraitement
	{
		Sondes
		{
		 seg_P pression periode 0.1 segment 10 0 0.25  1 0.25
		 seg_U1 vitesse periode 0.1 segment 10 0.5 0. 0.5 0.5
		 sonde_pression pression periode 0.1 points 1 0.5 0.425
		 sonde_vitesse  vitesse periode 0.1 points 1 0.5 0.425
		}
		format lml
		Champs dt_post 1.0
		{
			pression elem
			vitesse elem
			estimateur_aposteriori elem 
		}
	}

}
Solve pb
Fin
Fin
 