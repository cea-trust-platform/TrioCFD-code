# SIMULATION D UN TUYAU COUDE AVEC ECOULt TURBULENT Re=50000 #
dimension 3
PrecisionGeom 1.e-5 # Pour pouvoir relire les fichiers .dat #
Pb_Hydraulique_Turbulent pb

Domaine dom_DOM

Scatter dom_DOM.Zones dom_DOM

VEFPreP1B dis

schema_euler_implicite sch
Lire sch
{
	tinit 0.
	tmax 0.5 # Provisoire #
	dt_impr 0.001
	dt_sauv 400.
	seuil_statio 1.e-3
	Residuals { relative 1 }
	facsec 30. 
	facsec_max 300. 
	Solveur Implicite { 
	  solveur gmres { diag seuil 1.e-13 impr controle_residu 1 nb_it_max 5 }
	}
}



Associer pb dom_DOM
Associer pb sch 
Discretiser pb dis

Lire pb
{
   
   fluide_incompressible {
    	mu Champ_Uniforme       1 1.90e-5
    	rho Champ_Uniforme      1 1.205
   }
   
   	Navier_Stokes_Turbulent	
	{
		solveur_pression petsc cholesky { } 
		convection { muscl }
		diffusion { }
		conditions_initiales 
			{
			vitesse champ_uniforme 3 0 0 0
			}
		conditions_limites {
			entree frontiere_ouverte_vitesse_imposee champ_front_lu dom_DOM 3 pb_VITESSE_perio.dat
			sortie frontiere_ouverte_pression_imposee Champ_front_uniforme 1 0.
			parois  paroi_fixe
			symmetry symetrie
		}
                 Modele_turbulence K_Epsilon {
			Transport_K_Epsilon
                        {
                         convection { amont } 
                         diffusion { } 
                         conditions_limites
                            {
			     entree frontiere_ouverte_k_eps_impose champ_front_lu dom_DOM 2 pb_K_EPS_perio.dat
			     sortie frontiere_ouverte K_EPS_EXT Champ_Front_Uniforme 2 0.405 7.73 # k=3/2(U*t)^2 (t=5%), eps=Cmu/0.07/D*k^1.5 #
                             parois  paroi
			     symmetry symetrie
                            }
                         conditions_initiales
                           {
                             k_Eps Champ_Uniforme 2 0.405 7.73 # k=3/2(U*t)^2 (t=5%), eps=Cmu/0.07/D*k^1.5 #
                           }
                        }
                        Turbulence_paroi  loi_standard_hydr dt_impr_ustar 3000
                }
	}
	Postraitement
	{
		Definition_champs
		{
			W Transformation 
			{
				methode formule expression 1 (Ux*cos(atan(x/y))-Uy*sin(atan(x/y)))*(y>0)+(Uy*(x[0)-Uy*(x>0))*(y[0)  
				sources {
					Transformation { methode composante numero 0 source refchamp { Pb_champ pb vitesse } nom_source Ux } ,
					Transformation { methode composante numero 1 source refchamp { Pb_champ pb vitesse } nom_source Uy }
				}
			} 
			# V_sur_AA = 0 par symetrie #
			V_sur_BB Transformation 
			{
				methode formule expression 1 (-Uy*cos(atan(x/y))-Ux*sin(atan(x/y)))*(y>0)+(Ux*(x[0)-Ux*(x>0))*(y[0)  
				sources {
					Transformation { methode composante numero 0 source refchamp { Pb_champ pb vitesse } nom_source Ux } ,
					Transformation { methode composante numero 1 source refchamp { Pb_champ pb vitesse } nom_source Uy }
				}
			} 
			# U_sur_BB = Uz mais non demande #
			# U_sur_AA assez complique a calculer et non demande #
		}
