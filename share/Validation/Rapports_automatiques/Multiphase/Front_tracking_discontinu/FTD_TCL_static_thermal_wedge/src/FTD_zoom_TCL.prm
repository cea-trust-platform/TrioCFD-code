	visu {
		Title "Initial and final states"
		nb_img_without_newline 2
		Width 7.5cm
		instruction_visit DefineScalarExpression("Tl_ELEM_dom","if(or(INDICATRICE_INTERF_ELEM_dom=1, INDICATRICE_INTERF_ELEM_dom=0), TEMPERATURE_THERMIQUE_ELEM_dom*INDICATRICE_INTERF_ELEM_dom,-(INDICATRICE_INTERF_ELEM_dom)^2/2*TEMPERATURE_GRAD_THERMIQUE_ELEM_dom*1.e-5)")
		instruction_visit DefineVectorExpression("gradT_ELEM_dom", "if(gt(INDICATRICE_INTERF_ELEM_dom, 0.5), gradient(TEMPERATURE_THERMIQUE_ELEM_dom), {0,0,0})")
		Origin Visit
		mesh REF/lata/post.lata dom
		mesh REF/lata/post.lata INTERFACES
		Pseudocolor REF/lata/post.lata dom Tl ELEM
		BlackVector REF/lata/post.lata dom gradT ELEM 0.09
		instruction_visit VectorAtts = VectorAttributes()
		instruction_visit VectorAtts.useStride = 1
		instruction_visit VectorAtts.stride = 1
		instruction_visit VectorAtts.scale = 0.09
		instruction_visit VectorAtts.scaleByMagnitude = 0
		instruction_visit try: VectorAtts.colorByMagnitude = 0
		instruction_visit except: VectorAtts.colorByMag = 0
		instruction_visit SetPlotOptions(VectorAtts)
		cycles 0 -1
		normal3D 0. 0. 1. 
		up3D 0. 1 0.
	}
	visu {
		Title "Temperature Difference : dT = Tl - Tana"
		nb_img_without_newline 1
		Width 7.5cm
		instruction_visit DefineScalarExpression("Tl_ELEM_dom","if(or(INDICATRICE_INTERF_ELEM_dom=1, INDICATRICE_INTERF_ELEM_dom=0), TEMPERATURE_THERMIQUE_ELEM_dom*INDICATRICE_INTERF_ELEM_dom,-(INDICATRICE_INTERF_ELEM_dom)^2/2*TEMPERATURE_GRAD_THERMIQUE_ELEM_dom*1.e-5)")
		instruction_visit DefineVectorExpression("gradT_ELEM_dom", "if(gt(INDICATRICE_INTERF_ELEM_dom, 0.5), gradient(TEMPERATURE_THERMIQUE_ELEM_dom), {0,0,0})")
		instruction_visit DefineScalarExpression("Tana_ELEM_dom", "if(or(INDICATRICE_INTERF_ELEM_dom=1, INDICATRICE_INTERF_ELEM_dom=0),7.*(1-180/3.14159*atan(coord(dom)[1]/(abs(coord(dom)[0]-5.e-6-(0.0e-6))+1.e-9)/50))*INDICATRICE_INTERF_ELEM_dom,-(INDICATRICE_INTERF_ELEM_dom)^2/2*TEMPERATURE_GRAD_THERMIQUE_ELEM_dom*1.e-5)")
		instruction_visit DefineScalarExpression("dT_ELEM_dom", "Tl_ELEM_dom-Tana_ELEM_dom")

		Origin Visit
		mesh REF/lata/post.lata dom
		mesh REF/lata/post.lata INTERFACES
#		Operator_to_all no_legend
		Operator no_databaseinfo
		Pseudocolor_with_range REF/lata/post.lata dom dT ELEM min max
		Vector REF/lata/post.lata dom gradT ELEM 0.4
		instruction_visit VectorAtts = VectorAttributes()
		instruction_visit VectorAtts.useStride = 1
		instruction_visit VectorAtts.stride = 1
		instruction_visit VectorAtts.scale = 0.4
		instruction_visit VectorAtts.scaleByMagnitude = 1
		instruction_visit try: VectorAtts.colorByMagnitude = 1
		instruction_visit except: VectorAtts.colorByMag = 1
		instruction_visit SetPlotOptions(VectorAtts)
		cycles -1
		normal3D 0. 0. 1. 
		up3D 0. 1 0.
	}
	
	visu {
		Title "final comparison of mp to analytical using VisIt projection by pos_cmfe"
		nb_img_without_newline 1
		Origin Visit
		Width 7.5cm
		instruction_visit DefineScalarExpression("ri","1.e-20+sqrt((coord(INTERFACES)[0]-5e-6)^2+(coord(INTERFACES)[1])^2)")
		instruction_visit DefineScalarExpression("mpana", "0.67897/(-2.2574e6)*7.0/(ri*(50*3.14159)/180)")
		instruction_visit DefineScalarExpression("mp", "pos_cmfe(<[0]id:MPOINT_THERMIQUE_ELEM_dom>,INTERFACES,-100.)")
		instruction_visit  OpenDatabase("REF/lata/post.lata")
		instruction_visit ScatterAtts = ScatterAttributes()
		instruction_visit ScatterAtts.var1 = "ri"
		instruction_visit ScatterAtts.var2 = "mpana"
		instruction_visit ScatterAtts.colorType = ScatterAtts.ColorBySingleColor
		instruction_visit ScatterAtts.pointSizePixels = 10
		instruction_visit ScatterAtts.singleColor = (0, 0, 0, 255)
		instruction_visit ScatterAtts.scaleCube = 0
		instruction_visit SetDefaultPlotOptions(ScatterAtts)
		instruction_visit AddPlot("Scatter", "mpana", 0, 1)
		instruction_visit SetPlotOptions(ScatterAtts)
		instruction_visit AddPlot("Scatter", "mp", 0, 1)
		instruction_visit ScatterAtts.var2 = "mp"
		instruction_visit ScatterAtts.singleColor = (255, 0, 0, 255)
		instruction_visit SetPlotOptions(ScatterAtts)
		instruction_visit DrawPlots()
		instruction_visit View2DAtts = View2DAttributes()
		instruction_visit View2DAtts.windowCoords = (5.20315e-08, 1.56526e-05, -9.20733, -0.154104)
		instruction_visit View2DAtts.viewportCoords = (0.2, 0.95, 0.15, 0.95)
		instruction_visit SetView2D(View2DAtts)
		cycles -1
	}
	
