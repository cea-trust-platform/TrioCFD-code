# Test du couplage two_way_coupling (VEF) #
# Lacher de particules (10000) dans une colonne ou circule un fluide #
# PARALLEL OK #

dimension 3

Probleme_FT_Disc_gen pb
Domaine dom

# BEGIN MESH #

Mailler dom
{
	Pave Cavite 
		{
		Origine 0. 0. 0.
     		Nombre_de_Noeuds 3 3 11
		Longueurs 0.2 0.2 1.
		}
		{
		Bord Gauche   X = 0.    0. <= Y <= 0.2  0. <= Z <= 1.
		Bord Haut     Z = 1.    0. <= X <= 0.2  0. <= Y <= 0.2
		Bord Bas      Z = 0.    0. <= X <= 0.2  0. <= Y <= 0.2
		Bord Droit    X = 0.2   0. <= Y <= 0.2  0. <= Z <= 1.
		Bord Avant    Y = 0.    0. <= X <= 0.2  0. <= Z <= 1.
		Bord Arriere  Y = 0.2   0. <= X <= 0.2  0. <= Z <= 1.
		
		}
}

Tetraedriser_homogene dom

Sous_Zone zone1
Associate zone1 dom
Read zone1 {
   Boite
     Origine 0. 0. 0.8
     Cotes   0.2 0.2 0.2
}


# END MESH #

# BEGIN PARTITION
Partition dom
{
	Partitionneur tranche { tranches 1 1 2 }
	Larg_joint 2
	Nom_Zones DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
Sous_Zone zone1
Associate zone1 dom
Read zone1 { Fichier zone1.ssz }
END SCATTER #

# Je choisis une discretisation #
VEFPreP1b ma_discretisation

Schema_Euler_explicite mon_schema
Read mon_schema
{
	tinit 0.
	tmax 1.
	dt_min 5.e-5
	dt_max 5.e-5
	dt_impr 5.e-3
	dt_sauv 100
	seuil_statio -1.
	nb_pas_dt_max 20000
}

Associate pb dom
Associate pb mon_schema
Discretize pb ma_discretisation

Read pb
{
								solved_equations
	{
Navier_Stokes_FT_Disc          hydraulique
Transport_Marqueur_FT	       marqueurs
	}
	Fluide_Incompressible 
{
	mu  Champ_Uniforme 1 3.7e-05
 	rho Champ_Uniforme 1 2
 	gravite Champ_Uniforme 3  0. 0. 0.
}
	hydraulique
	{
		modele_turbulence sous_maille_wale
        	{
           	  Cw 0.
          	  turbulence_paroi negligeable
        	}
		
		solveur_pression GCP { 
			precond ssor { omega 1.5 } 
			seuil 1.e-8
			
		}
		convection { muscl }
		diffusion { }
		conditions_initiales {
			vitesse Champ_Uniforme 3 0. 0. -1
			}
		
		boundary_conditions {
			Gauche  paroi_fixe
			Droit   paroi_fixe
			Avant   paroi_fixe
			Arriere paroi_fixe
			Haut    frontiere_ouverte_vitesse_imposee Champ_front_Uniforme 3 0. 0. -1
			Bas     Sortie_libre_rho_variable champ_front_uniforme 1 0. 
			
		}
	        
	}
	
        
	marqueurs
	{
	 conditions_initiales {
	        ensemble_points { sous_zones 1 zone1 aleatoire 10000 } 
		proprietes_particules { 
					distribution {
					nb_particules 10000
		       			vitesse 0. 0. 0.
					temperature 273.
					masse_volumique 3.
					diametre 0.01
					}
		      		       }
		t_debut_integration 0.
	 }
	 boundary_conditions
		{
		      
		}
         sources { Trainee }	
	 methode_transport vitesse_particules	
	 methode_couplage two_way_coupling 
	
	}
	
	Postraitement
	{
		Sondes
		{
			sonde_pression_p pression periode 0.05 points 1 0.105 0.105 0.5
			sonde_vit_p vitesse periode 0.05 points 1 0.105 0.105 0.5
			sonde_vit_c_vert vitesse periode 0.05 segment 20 0.105 0.105 0. 0.105 0.105 1.
			sonde_vit_c_hori vitesse periode 0.05 segment 20 0. 0.105 0.5 0.2 0.105 0.5
			sonde_dens_p densite_particules periode 0.05 points 1 0.105 0.105 0.5
			sonde_dens_c_vert densite_particules periode 0.05 segment 20 0.105 0.105 0. 0.105 0.105 1.
			sonde_dens_c_hori densite_particules periode 0.05 segment 20 0. 0.105 0.5 0.2 0.105 0.5
						
		}
        	Format lata
		Champs dt_post 0.05
		{
			pression elem
			pression som
			vitesse elem
			vitesse som
			densite_particules elem
			volume_particules elem
			gradient_pression elem
		}
		
	}
liste_postraitements
{	
        postraitement_ft_lata marqueurs { 
    format Lata 
    fichier marqueurs 
    champs dt_post 0.05 { 
        vitesse som 
    } 
    interfaces marqueurs { 
    } 
}	
}
}

Solve pb
End

 