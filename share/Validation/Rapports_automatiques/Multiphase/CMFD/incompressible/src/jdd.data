# PARALLEL RUNS #

Dimension 3
Domaine dom

# BEGIN MESH #
read_med { domain dom mesh mesh file ../1_canal_1.med }
# END MESH #

# BEGIN PARTITION
Partition dom
{
        /* Choose Nb_parts so to have ~ 25000 cells per processor */
        Partition_tool metis { nb_parts 4 }
        Larg_joint 2
        zones_name DOM
}
End
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
END SCATTER #

PolyMAC_P0  dis
option_PolyMAC_P0 { traitement_axi }


Schema_euler_implicite sch
Read sch
{
    tinit 0
    tmax 10 
    dt_impr 1e-8
    facsec 1e30
    nb_pas_dt_max $nb_pas_dt_max
    seuil_statio 1.e-3
    dt_max 0.1
    solveur implicite
    {
        solveur petsc cli { }
        seuil_convergence_implicite 1e30
    }
}

Pb_thermohydraulique pb
Associate  pb dom
Associate  pb sch
Discretize pb dis

Lire pb
{
	fluide_incompressible
    {
    	rho     champ_uniforme 1 $rho
        mu      champ_uniforme 1 $mu
        lambda  champ_uniforme 1 $lambda
        cp      champ_uniforme 1 $cp
        beta_th champ_uniforme 1 $beta
        gravite champ_uniforme 3 0 0 -9.81
    }

    correlations
    {
    	loi_paroi adaptative { }
    	flux_parietal  adaptatif
    }
    navier_stokes_standard
    {
        solveur_pression petsc cholesky { }
        convection { amont }
        diffusion  { turbulente k_omega { }  }
        initial_conditions
        {
            vitesse  champ_fonc_t 3 0. 0. $u_0
        }
        conditions_limites	
        {
            wall_hot  paroi_frottante_loi { } 
            wall_cold paroi_frottante_loi { }
            bottom frontiere_ouverte_vitesse_imposee_sortie Champ_front_Fonc_t 3 0 0 $u_0
            top frontiere_ouverte_pression_imposee champ_front_uniforme 1 $P0
            symetrie symetrie
        }
	    sources
        {
            boussinesq_temperature { T0  $Tl0 verif_boussinesq  0 }
        }
    }
    convection_diffusion_temperature
    {
        diffusion { turbulente SGDH { Pr_t 0.9 } }
        convection { amont }
        initial_conditions { temperature Champ_Uniforme 1 $Tl0  }
        boundary_conditions
        {
            wall_hot  neumann_paroi champ_front_fonc_t 1 $P_surf
            wall_cold paroi_adiabatique	    
            bottom	frontiere_ouverte T_ext Champ_Front_Uniforme 1 $Tl0
            top	frontiere_ouverte T_ext Champ_Front_Uniforme 1 $Tl0
            symetrie paroi_adiabatique
        }
    }

    taux_dissipation_turbulent
    {
        diffusion { turbulente SGDH { sigma 0.5 } }
        convection { amont }
        initial_conditions { omega Champ_Fonc_xyz dom 1 $om0 }
        boundary_conditions
        {
            wall_hot  Cond_lim_omega_demi {  }
            wall_cold Cond_lim_omega_demi {  }	    
            bottom	frontiere_ouverte omega_ext Champ_Front_Uniforme 1 $om0
            top	frontiere_ouverte omega_ext Champ_Front_Uniforme 1 $om0
            symetrie paroi
        }
        sources
        {
            Production_echelle_temp_taux_diss_turb { alpha_omega 0.5 } ,
            Dissipation_echelle_temp_taux_diss_turb { beta_omega 0.075 }  ,
            Diffusion_croisee_echelle_temp_taux_diss_turb { sigma_d 0.5 } 
        }
    }


    energie_cinetique_turbulente
    {
        diffusion { turbulente SGDH { sigma 0.67 } }
        convection { amont }
        initial_conditions { k champ_fonc_xyz dom 1 $k0 }
        boundary_conditions
        {
            wall_hot  Cond_lim_k_complique_transition_flux_nul_demi
            wall_cold Cond_lim_k_complique_transition_flux_nul_demi	    
            bottom	frontiere_ouverte k_ext Champ_Front_Uniforme 1 $k0
            top	frontiere_ouverte k_ext Champ_Front_Uniforme 1 $k0
            symetrie paroi
        }
        sources
        {
            Production_energie_cin_turb { } ,
            Terme_dissipation_energie_cinetique_turbulente { beta_k 0.09 }
        }
    }
    Postraitement
	{
		sondes		{
					temperature temperature periode 1e8 segment 20 0.00023996344683753387  4.188577544948042e-06 4.485 0.00935857442666382 0.00016335452425297366  4.485 
					vitesse vitesse periode 1e8 segment 20 0.00023996344683753387  4.188577544948042e-06 4.485 0.00935857442666382 0.00016335452425297366  4.485 
					}
		format lata
		champs binaire dt_post 1.e8
					{
					temperature elem
					vitesse elem
					pression elem
                    k elem
                    omega elem
                    NU_TURBULENTE elem
                    omega_residu elem
                    k_residu elem
                    vitesse_residu elem
                    temperature_residu elem
					}
	}
}

Resoudre pb

Fin
