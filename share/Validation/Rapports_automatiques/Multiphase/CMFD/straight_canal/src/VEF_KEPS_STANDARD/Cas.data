                sources { refChamp { pb_champ pb k_eps } }
            }
            conv_keps  operateur_eqn  {
                numero_op 1
                sources { refChamp { pb_champ pb k_eps } }
            }
            sources_keps_elem Interpolation  {
                localisation elem
                source  operateur_eqn {
                    numero_source 0
                    sources { refChamp { pb_champ pb k_eps } }
                }
            }
            diff_keps_elem    Interpolation  {
                localisation elem
                source  operateur_eqn {
                    numero_op 0
                    sources { refChamp { pb_champ pb k_eps } }
                }
            }
            conv_keps_elem    Interpolation  {
