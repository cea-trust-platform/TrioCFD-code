    smooth_steps 1 7
    relax_jacobi 1 0.7
    solveur_grossier GCP { seuil 1e-5 precond ssor { omega 1.5 }  }  # 8 # # 1.e-3 au premier pas de temps post switch, puis 1e-8 #
    seuil 1e-3 # 7 # # 1.e-2 au premier pas de temps post switch, puis 1.e-7 #
    nb_full_mg_steps 2 20 2
    impr
    iterations_mixed_solver 10
  }
  
