# aaa #
# lance_test 1 ecarts #

dimension 3

domaine dom_pb1

Mailler dom_pb1
{
	Pave Entree 
	{
	  Origine 0. 0. 0.

	  Nombre_de_Noeuds 5 11 5
	  Longueurs 1 0.2 0.1
	}
	{
	  Bord Entree X = 0.   0. <= Y <= 0.2 0. <= Z <= 0.1      
	  Bord Haut   Y = 0.2   0. <= X <= 1 0. <= Z <= 0.1 
	  Bord Bas    Y = 0.   0. <= X <= 1   0. <= Z <= 0.1 
	  Bord Entree X = 1   0. <= Y <= 0.2 0. <= Z <= 0.1 
	  Bord Devant Z = 0.  0. <= X <= 1 0. <= Y <= 0.2 
	  Bord Devant Z = 0.1   0. <= X <= 1 0. <= Y <= 0.2
	}
}

Pb_hydraulique  pb1

Schema_Euler_implicite sch
Read sch
{
        tinit 0.
	nb_pas_dt_max 3
        tmax 1000.
        dt_min 1.e-8
        dt_max 100000.
        dt_impr 100.
        dt_sauv 500.
        seuil_statio 1.e-8
        facsec 1.
	facsec_max 200

	Solveur Implicite
	{
		seuil_convergence_solveur 1.e-13
		solveur gmres { diag seuil 1.e-13 impr controle_residu 1 nb_it_max 5 }
	}
}

VDF cubesregu


Associate pb1 dom_pb1
 
Associate pb1 sch

Discretize pb1 cubesregu

Read pb1
{
   
   fluide_incompressible {
   
        gravite champ_uniforme 3 0. 0. 0
   	    mu      Champ_Uniforme  1 3.56e-5
   	    rho     Champ_Uniforme  1 3.85
   }
   
           Navier_Stokes_standard
        {
                solveur_pression GCP { 
                        precond ssor { omega 1.5 }
                        seuil 1.e-07 
                }
		solveur_pression petsc cholesky  { }
		convection { amont }
                diffusion { }
       		Sources { Canal_perio { bord Entree }  }
                conditions_initiales {
                        vitesse Champ_Uniforme 3 0.05 0. 0.
                           }
                boundary_conditions {
                        Entree          periodique 
                        Haut            paroi_fixe
                        Bas             paroi_fixe
                        Devant          periodique 
               }
		Traitement_particulier { Canal { dt_impr_moy_spat 300. }
                                        }
        }

        Postraitement {
	format lml
                Champs dt_post 300.
                {
		   pression elem
                   vitesse elem
                }
        }
}
EcritureLectureSpecial 0
Solve pb1
End

 
