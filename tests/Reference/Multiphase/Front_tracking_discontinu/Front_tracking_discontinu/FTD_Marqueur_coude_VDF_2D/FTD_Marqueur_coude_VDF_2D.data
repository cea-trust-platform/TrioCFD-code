# Cas test Front-tracking discontinu VDF 2D #
# Interface solide a l exterieur du domaine #
# Presence d un marqueur #
# Geometrie d un coude avec maillage irregulier #
# PARALLEL OK #

dimension 2

Probleme_FT_Disc_gen pb
Domaine dom

# BEGIN MESH #
Mailler dom
{
	Pave Entree 
	{
	Origine 0. 0.
	Nombre_de_Noeuds 11 11
	Longueurs 1.0 1.0
	Facteurs 1.3 0.8
	SYMX
	}
	{
	Bord Entree Y = 0. 0. <= X <= 1.
	Bord Ouest  X = 0. 0. <= Y <= 1.
	Bord Est    X = 1. 0. <= Y <= 1.
	} ,
	Pave Char
	{
	Origine 0. 1.
	Nombre_de_Noeuds 11 11
	Longueurs 1.0 1.0
	Facteurs 1.3 1.3
	SYMX  SYMY
	}
	{
	Bord Haut1  Y = 2. 0. <= X <= 1.
	Bord Ouest1 X = 0. 1. <= Y <= 2.
	} ,	
	Pave Sortie
	{
	Origine 1. 1.
	Nombre_de_Noeuds 11 11
	Longueurs 1.0 1.0
	Facteurs 1.2 1.3
	SYMY	
	}
	{
	Bord Sortie X = 2. 1. <= Y <= 2.
	Bord Haut   Y = 2. 1. <= X <= 2.
	Bord Bas    Y = 1. 1. <= X <= 2.
	}
}

Sous_Zone zone1
Associate zone1 dom
Lire zone1 {
   Rectangle
     Origine 0.02 0.1
     Cotes 0.96 0.3
}


# END MESH #
# BEGIN PARTITION
Partition dom
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 1
	Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
Sous_Zone zone1
Associate zone1 dom
Lire zone1 { Fichier zone1.ssz }
END SCATTER #

VDF dis

Schema_Euler_explicite sch
Lire sch
{
	tinit 0.
	tmax 3.  
	dt_min 0.00001
	dt_max 10.     
	dt_impr 0.00001
	dt_sauv 100
	seuil_statio 1e-8
	nb_pas_dt_max 20
}

Associate pb dom
Associate pb sch
Discretize pb dis

Lire pb
{
		    solved_equations
    {
Navier_Stokes_FT_Disc          hydraulique
Transport_Marqueur_FT	       marqueurs
    }
	Fluide_Incompressible 
{
	mu Champ_Uniforme 1 0.02
	rho Champ_Uniforme 1 2
	gravite Champ_Uniforme 2 0. -9.81
}

	hydraulique
	{
		modele_turbulence sous_maille_wale
        	{
           	  Cw 0.
          	  turbulence_paroi negligeable
        	}
		
		solveur_pression GCP { 
			precond ssor { omega 1.5 } 
			seuil 1.000000e-10 
			impr 
		}
		convection { amont }
		diffusion { }
		conditions_initiales {
		        vitesse Champ_Uniforme 2 0. 0.
			}
		
		boundary_conditions {
			Haut paroi_fixe
			Haut1 paroi_fixe
			Bas paroi_fixe
			Ouest1 paroi_fixe 
			Ouest paroi_fixe
			Est paroi_fixe
			Sortie Sortie_libre_rho_variable champ_front_uniforme 1 0.
			Entree frontiere_ouverte_vitesse_imposee Champ_Front_Uniforme 2  0. 1.
                }
	}
	
	
	marqueurs
	{
	 conditions_initiales {
	     ensemble_points { sous_zones 1 zone1 uniforme 50 15 } 
	 }
	 boundary_conditions
		{
		        
		}
	}
	Postraitement
	{
		Sondes
		{
			sonde_pression pression periode 0.1 points 2 0.45 1.45 0.45 1.55
			sonde_vitesse vitesse periode 0.1 points 1 9.44709226e-01 0.112030
			segment_vitesse grav vitesse periode 0.1 segment 10 0. 1. 1. 1.0
		}
		
		
		Champs dt_post 1.e-2
		{
			pression elem
			vitesse som
			vitesse elem		
			densite_particules elem
		}
	}	
	liste_postraitements
	{
	       postraitement_ft_lata marqueurs { 
    format Lata 
    fichier marqueurs 
    Champs dt_post 1.e-2
		{	
			densite_particules elem
		}
    interfaces marqueurs { 
        vitesse som 
    } 
}	
	}
}

Solve pb
Fin
 
