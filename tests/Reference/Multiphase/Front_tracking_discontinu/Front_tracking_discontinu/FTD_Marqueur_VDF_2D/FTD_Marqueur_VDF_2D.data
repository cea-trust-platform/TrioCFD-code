# Cas test Front-tracking discontinu VDF 2D.
  Cas test avec interface liquide-vapeur "interf"
                concentration
  Interface liquide-vapeur initiale : un demi-plan
  gravite, tension superficielle.
  Presence d un marqueur #
# PARALLEL OK #

dimension 2
Probleme_FT_Disc_gen pb
Domaine DOM

# BEGIN MESH #
Mailler DOM
{
	Pave pave1
	{
		origine 0. 0.
		longueurs 0.04 0.04 
		nombre_de_noeuds 11 11
	}
	{
		bord entree X = 0.   0. <= Y <= 0.04 
		bord sortie X = 0.04 0. <= Y <= 0.04 
		bord paroi  Y = 0.   0. <= X <= 0.04 
		bord paroi  Y = 0.04 0. <= X <= 0.04 
	}
}


Sous_Zone zone1
Associate zone1 DOM
Lire zone1 {
   Rectangle
     Origine 0.002 0.002
     Cotes 0.0056 0.0056 
}


Sous_Zone zone2
Associate zone2 DOM
Lire zone2 {
   Rectangle
     Origine 0.03 0.03
     Cotes 0.005 0.005 
}

Sous_Zone zone3
Associate zone3 DOM
Lire zone3 {
   Rectangle
     Origine 0.015 0.015
     Cotes  0.01 0.01
}

Sous_Zone zone_transfo1
Associate  zone_transfo1 DOM
Lire  zone_transfo1 {
   Rectangle
     Origine 0.016 0.008
     Cotes   0.008 0.004 
}

# END MESH #
# BEGIN PARTITION
Partition DOM
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 2
	Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones DOM
Sous_Zone zone1
Associate zone1 DOM
Lire zone1 { Fichier zone1.ssz }
Sous_Zone zone2
Associate zone2 DOM
Lire zone2 { Fichier zone2.ssz }
Sous_Zone zone3
Associate zone3 DOM
Lire zone3 { Fichier zone3.ssz }
Sous_Zone zone_transfo1
Associate zone_transfo1 DOM
Lire zone_transfo1 { Fichier zone_transfo1.ssz }
END SCATTER #

VDF dis

Schema_Euler_explicite  sch
Lire sch
{
	tinit 0.
	tmax  1.
	dt_min 1.e-7
	dt_max 0.5e-2
	dt_impr 10.
	dt_sauv 100
	seuil_statio -1
	nb_pas_dt_max 30
}

Associate pb DOM
Associate pb sch
Discretize pb dis

Lire pb
{
	    solved_equations
    {
Navier_Stokes_FT_Disc              hydraulique
Transport_Interfaces_FT_Disc       interf
Transport_Marqueur_FT	       	   marqueurs
Convection_Diffusion_Concentration concentration
    }
	Fluide_Diphasique 
{
	fluide0 Fluide_Incompressible 
{
	mu  Champ_Uniforme 1 0.282e-3
 	rho Champ_Uniforme 1 1000.
}

	fluide1 Fluide_Incompressible 
{
	mu  Champ_Uniforme 1 0.282e-3
 	rho Champ_Uniforme 1 100.
}
	sigma   Champ_Uniforme 1 0.05
	gravite Champ_Uniforme 2  0. -9.81
}

Constituant 
{
	coefficient_diffusion Champ_Uniforme 1 1e-6
}

hydraulique
{
	modele_turbulence sous_maille_wale
        {
           Cw               0.
           turbulence_paroi negligeable
        }

	solveur_pression GCP { precond ssor { omega 1.5 } seuil 1e-13 impr }
	convection           { quick }
	diffusion            { }
	conditions_initiales { vitesse champ_uniforme 2 0. 0. }
	equation_interfaces_proprietes_fluide interf
	boundary_conditions
	{
		sortie   Sortie_libre_rho_variable champ_front_uniforme 1 0.
		paroi    paroi_fixe
		entree   Frontiere_ouverte_vitesse_imposee champ_front_uniforme 2 0.1 0.0 
	}
}

interf
{
	methode_transport vitesse_interpolee hydraulique
	conditions_initiales {
		fonction x-0.005 

        }
       
	remaillage {
		pas -1
		pas_lissage -1
	}
	
	boundary_conditions
	{
		paroi  Paroi_FT_disc symetrie
		sortie Paroi_FT_disc symetrie
		entree Paroi_FT_disc symetrie
	}
}

marqueurs
	{
	 conditions_initiales {
	     ensemble_points { fichier particules }
	    
	    t_debut_integration 0.
	 }
	 
	 boundary_conditions
		{
		 
		}
	transformation_bulles
	 {
	   
	   interface interf
	    
	 }
	 phase_marquee 1
	}

concentration
{
	diffusion { negligeable }
	convection { quick }
	conditions_initiales { concentration champ_fonc_xyz DOM 1 EXP(-((x-0.02)^2+(y-0.02)^2/0.03^2)) }
	boundary_conditions {
		sortie  frontiere_ouverte C_ext Champ_Front_Uniforme 1 0.
		paroi   paroi
		entree  frontiere_ouverte C_ext Champ_Front_Uniforme 1 1.
	}
}

Postraitement
{
	Format lml
	champs dt_post 0.005
	{
		indicatrice_interf
		concentration
		densite_particules elem
		vitesse elem
		pression_pa elem
	}
}
liste_postraitements
{
	postraitement_ft_lata post1 { 
    format Lata 
    fichier post1 
    champs dt_post 0.05 { 
        vitesse elem 
        distance_interface_elem_interf elem 
        indicatrice_interf elem 
        pression elem 
        concentration elem 
    } 
    interfaces interf { 
        courbure som 
        pe elem 
    } 
}
	postraitement_ft_lata marqueurs { 
    format Lata 
    fichier marqueurs 
    champs dt_post 0.05 { 
        vitesse elem 
        distance_interface_elem_interf elem 
        indicatrice_interf elem 
        pression elem 
        concentration elem 
    } 
    interfaces marqueurs { 
        vitesse som 
    } 
}	
}


}

Solve pb
Fin
 