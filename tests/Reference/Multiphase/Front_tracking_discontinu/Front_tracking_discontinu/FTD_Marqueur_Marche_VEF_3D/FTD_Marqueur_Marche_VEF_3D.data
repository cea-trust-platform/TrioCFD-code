# Cas test Front-tracking discontinu VEFPreP1b #
# Interface solide a l exterieur du domaine #
# Presence d un marqueur #
# Geometrie de la marche #
# PARALLEL OK #

dimension 3
Probleme_FT_Disc_gen pb
Domaine dom
# BEGIN MESH #
Read_file Marche3D_marq_large.geo ;
Tetraedriser_homogene dom

Sous_Zone zone1
Associate zone1 dom
Lire zone1 {
    Boite
    Origine 0. 1. 0.
    Cotes  3  0.6 10
}
Sous_Zone zone2
Associate zone2 dom
Lire zone2 {
    Boite
    Origine 7. 0. 0.
    Cotes  3  1. 10
}

# END MESH #

# BEGIN PARTITION
Partition dom
{
    Partitionneur tranche { tranches 2 1 1 }
    Larg_joint 2
    Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
Sous_Zone zone1
Associate zone1 dom
Lire zone1 { Fichier zone1.ssz }
Sous_Zone zone2
Associate zone2 dom
Lire zone2 { Fichier zone2.ssz }
END SCATTER #

VEFPreP1b dis

schema_Euler_explicite sch
Lire sch
{
    tinit 0.
    tmax 1.5
    dt_min 1.e-6
    dt_max 0.2
    dt_impr 0.2
    dt_sauv 100.
    seuil_statio 1.e-8
    nb_pas_dt_max 3
}

Associate pb dom
Associate pb sch
Discretize pb dis

Lire pb
{
    solved_equations
    {
        Navier_Stokes_FT_Disc          hydraulique
        Transport_Marqueur_FT	       marqueurs
    }
    Fluide_Incompressible
    {
        mu Champ_Uniforme 1 5e-05
        rho Champ_Uniforme 1 2
        gravite Champ_Uniforme 3 0. -9.81 0
    }
    hydraulique
    {
        modele_turbulence sous_maille_wale
        {
            Cw               0.
            turbulence_paroi negligeable
        }
        solveur_pression GCP { precond ssor { omega 1.5 } seuil 1.e-10 }
        convection { amont }
        diffusion { }

        conditions_initiales {
            vitesse Champ_Uniforme 3  0. 0. 0.
        }
        boundary_conditions {
            Bas1 Paroi_Fixe
            Haut1 Paroi_Fixe
            Haut2 Paroi_Fixe
            Haut3 Paroi_Fixe
            Bas2 Paroi_Fixe
            Gauche Paroi_Fixe
            Bas3 Paroi_Fixe
            Sud1 Paroi_Fixe
            Nord1 Paroi_Fixe
            Sud2 Paroi_Fixe
            Nord2 Paroi_Fixe
            Sud3 Paroi_Fixe
            Nord3 Paroi_Fixe
            Sud4 Paroi_Fixe
            Nord4 Paroi_Fixe
            Sud5 Paroi_Fixe
            Nord5 Paroi_Fixe
            SortieBasse Sortie_libre_rho_variable champ_front_uniforme 1 0.
            SortieHaute Sortie_libre_rho_variable champ_front_uniforme 1 0.
            Entree frontiere_ouverte_vitesse_imposee Champ_Front_Uniforme 3  1. 0. 0.
        }

    }
    marqueurs
    {
        conditions_initiales {
            ensemble_points { sous_zones 2 zone1 aleatoire 1000  zone2 aleatoire 1000 }
        }
        boundary_conditions
        {

        }
    }
    Postraitements
    {
        lml
        {
            Sondes
            {
                sonde_pression pression periode 0.5 points 1 7.5 0.9 5.5
                sonde_vitesse vitesse periode 0.5 points 1 8.0 0.9 5.5

            }
            fichier FTD_Marqueur_Marche_VEF_3D
            Champs dt_post 0.5
            {
                pression elem
                pression som
                vitesse elem
                vitesse som

                densite_particules elem
            }
        }
        lata
        {
            fichier FTD_Marqueur_Marche_VEF_3D
            format lata
            Champs dt_post 0.5
            {
                pression elem
                pression som
                vitesse elem
                vitesse som
                densite_particules elem
            }

        }
    }
    liste_postraitements
    {
        postraitement_ft_lata marqueurs {
            format Lata
            fichier marqueurs
            Champs dt_post 0.5
            {
                densite_particules elem
            }
            interfaces marqueurs {
                vitesse som
            }
        }
    }
}
Solve pb
Fin

