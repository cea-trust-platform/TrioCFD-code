# Hydraulique 2D VDF : Calcul quasi-compressible iso-therme #
# Presence d une interface mobile solide #
# Interface en rotation a vitesse angulaire constante (egale a l unite) #
# Presence d un marqueur #
# PARALLEL OK #

dimension 2
domaine dom
Option_VDF
{
    traitement_coins oui
    P_imposee_aux_faces oui
}

# BEGIN MESH #
mailler dom
{
    pave Entree
    {
        Origine 0.0 0.0
        longueurs 1.0 1.0
        nombre_de_noeuds 61 60
    }

    {
        bord Entree  X = 0.0 0.0 <= Y <= 1.0
        bord Haut    Y = 1.0 0.0 <= X <= 1.0
        bord Bas     Y = 0.0 0.0 <= X <= 1.0
        bord Sortie  X = 1.0 0.0 <= Y <= 1.0
    }
}

Sous_Zone zone1
Associate zone1 dom
Lire zone1 {
    Rectangle
    Origine 0.2 0.4
    Cotes 0.1 0.1
}
# END MESH #

# BEGIN PARTITION
Partition dom
{
    Partitionneur tranche   { tranches 2 1   }
    Larg_joint 1
    Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
Sous_Zone zone1
Associate zone1 dom
Lire zone1 { Fichier zone1.ssz }
END SCATTER #

vdf dis

schema_euler_explicite sch
Lire sch
{
    tinit 0.0
    tmax 5000.0
    dt_min 0.000001
    dt_max 1.0
    dt_start dt_calc
    dt_sauv 100.0
    dt_impr 0.0001
    seuil_statio 1e-05
    nb_pas_dt_max 100
}




Pb_Thermohydraulique_Especes_QC pb
Associate pb dom
Associate pb sch
Discretize pb dis

Lire pb
{
    fluide_quasi_compressible
    {
        pression 171428.571429
        loi_etat gaz_parfait_qc
        {
            Cp 1000.0
            gamma 1.4
            Prandtl 1.0

        }
        traitement_pth constant
        traitement_rho_gravite moins_rho_moyen
        mu champ_uniforme  1 2.0
    }
    navier_stokes_qc
    {
        solveur_pression gcp
        {
            precond ssor
            {
                omega 1.65
            }
            seuil 1e-12
            impr
        }
        convection   { negligeable }
        diffusion    { }
        conditions_initiales
        {
            vitesse champ_uniforme  2 0.0 0.0
        }
        boundary_conditions
        {
            Haut paroi_fixe
            Bas paroi_fixe
            Entree frontiere_ouverte_pression_imposee champ_front_uniforme  1 0.
            Sortie frontiere_ouverte_pression_imposee champ_front_uniforme  1 0.
        }
    }
    convection_diffusion_chaleur_qc
    {
        convection   { negligeable }
        diffusion    { negligeable }
        conditions_initiales
        {
            temperature champ_uniforme  1 300.0
        }
        boundary_conditions
        {
            Haut paroi_adiabatique
            Bas paroi_adiabatique
            Entree frontiere_ouverte T_ext champ_front_uniforme  1 300.0
            Sortie frontiere_ouverte T_ext champ_front_uniforme  1 300.0
        }
    }
    Equations_Scalaires_Passifs
    {

        Transport_Interfaces_FT_Disc
        {
            conditions_initiales { fonction (1-((x-0.5)^2+(y-0.5)^2)/(0.1)^2) }
            remaillage {
                pas -1
                facteur_longueur_ideale 1
                pas_lissage 1e8
                nb_iter_barycentrage 2
                relax_barycentrage 1
            }
            boundary_conditions
            {
                Haut  Paroi_FT_disc symetrie
                Entree Paroi_FT_disc symetrie
                Sortie Paroi_FT_disc symetrie
                Bas   Paroi_FT_disc symetrie
            }
            methode_transport vitesse_imposee
            -(y-0.5)*1
            (x-0.5)*1
        }
        Transport_Marqueur_FT
        {
            conditions_initiales {
                ensemble_points { sous_zones 1 zone1 aleatoire 400 }
            }
            boundary_conditions
            {

            }

        }


    }

    postraitements
    {
        first_post {
            format lata
            sondes
            {
                seg_P pression periode 0.1 segment 20  0.025 0.5 0.975 0.5
                seg_U1 vitesse periode 0.1 segment 11  0.0 0.5 1.0 0.5
                seg_U2 vitesse periode 0.1 segment 19  1.0 0.02631 1.0 0.9736
                sonde_pression pression periode 0.1 points 1  0.5 0.5
                sonde_vitesse vitesse periode 0.1 points 1  0.5 0.5
            }
            champs dt_post 0.005
            {
                pression_pa elem
                pression_pa som
                vitesse elem
                vitesse som
                INDICATRICE_pbTransport_Interfaces_FT_Disc0 elem
            }
        }

        second_post {
            fichier Quasi_Comp_IBC_VDF_2D
            Format lml
            Sondes {
                sonde_vitesse_est vitesse periode 0.0001 point 1 0.59 0.5
                sonde_vitesse_ouest vitesse periode 0.0001 point 1 0.41 0.5
                sonde_vitesse_sud vitesse periode 0.0001 point 1 0.5 0.41
                sonde_vitesse_nord vitesse periode 0.0001 point 1 0.5 0.59
                sonde_vitesse_est_ouest vitesse periode 0.0005 segment 30 0. 0.5 1. 0.5
                sonde_vitesse_sud_nord vitesse periode 0.0005 segment 30 0.5 0. 0.5  1.
            }

            champs dt_post 0.005
            {
                pression_pa elem
                pression_pa som
                vitesse elem
                vitesse som
                temperature elem
                temperature som
                INDICATRICE_pbTransport_Interfaces_FT_Disc0 elem
                densite_particules elem
            }
        }
    }
    liste_postraitements
    {
        postraitement_ft_lata post1 {
            format Lata
            fichier post1
            champs dt_post 0.005
            {
                INDICATRICE_pbTransport_Interfaces_FT_Disc0 elem
            }
            interfaces pbtransport_interfaces_ft_disc0 {
            }
        }

    }
}
Solve pb
fin
boundary_conditions
{
    Haut  Paroi_FT_disc symetrie
    Entree Paroi_FT_disc symetrie
    Sortie Paroi_FT_disc symetrie
    Bas   Paroi_FT_disc symetrie
}

