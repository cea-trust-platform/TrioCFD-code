# Thermohydraulique 2D couplee a conduction 2D et zoom VDF-VDF #
# PARALLEL NOT #
dimension 2

Pb_conduction pb1
Pb_Thermohydraulique pb2

Domaine dom_solide
Domaine dom_fluide

# BEGIN MESH #
Mailler dom_solide
{
        Pave Cavite1
                {
                Origine 0. 0.
                Nombre_de_Noeuds 4 11
                Longueurs 0.3 1.0
                }
                {
                Bord Gauche1 X = 0.   0.  <= Y <= 1.0
                Bord Haut1   Y = 1.0  0.  <= X <= 0.3
                Bord Bas1    Y = 0.   0.  <= X <= 0.3
                Raccord local homogene Droit1  X = 0.3  0.3 <= Y <= 1.0
                } ,
        Pave Cavite2
                {
                Origine 0.3 0.
                Nombre_de_Noeuds 8 4
                Longueurs 0.7 0.3
                }
                {
                Raccord local homogene Haut2   Y = 0.3  0.3 <= X <= 1.0
                Bord Bas2    Y = 0.   0.3 <= X <= 1.0
                Bord Droit2  X = 1.0  0.  <= Y <= 0.3
                }
}

Mailler dom_fluide
{
        Pave Cavite3
                {
                Origine 0.3 0.3
                Nombre_de_Noeuds 15 15
                Longueurs 0.7 0.7
                }
                {
                Raccord local homogene Droit1f X = 0.3  0.3 <= Y <= 1.0
                Bord Entree    Y = 1.0  0.3 <= X <= 0.7
                Bord Haut3     Y = 1.0  0.7 <= X <= 1.0
                Raccord local homogene Haut2f Y = 0.3  0.3 <= X <= 1.0
                Bord Sortie    X = 1.0  0.3 <= Y <= 0.7
                Bord Droit2    X = 1.0  0.7 <= Y <= 1.0
                }
}
# END MESH #

# BEGIN PARTITION
Partition dom_fluide
{
	Partitionneur metis { Nb_parts 2 }
	Larg_joint 1
	Nom_Zones DOM1
}
Partition dom_solide
{
	Partitionneur metis { Nb_parts 2 }
	Larg_joint 1
	Nom_Zones DOM2
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM1.Zones dom_fluide
Scatter DOM2.Zones dom_solide
END SCATTER #


Schema_Euler_explicite sch1
Lire sch1
{
        tinit 0.
        tmax 300.
        dt_min 0.001
        dt_max 10.
        dt_impr 100
        dt_sauv 300.
        seuil_statio 1.e-8
}

Schema_Euler_explicite sch2
Lire sch2
{
        tinit 0.
        tmax 300.
        dt_min 0.001
        dt_max 10.
        dt_impr 100
        dt_sauv 300.
        seuil_statio 1.e-8
}

Pb_MG pbMG_global

VDF dis

# association pour le probleme grossier #
Associate pb1 dom_solide
Associate pb1 sch1

Discretize pb1 dis

# association pour le probleme fin #
Associate pb2 dom_fluide
Associate pb2 sch2

Discretize pb2 dis


# association multigrille #

Associer_pbMG_pbGglobal pbMG_global pb1

Associer_pbMG_pbFin pbMG_global pb2

Algo_Couple_1 algo
Lire algo
{ dt_uniforme }


Associer_algo pbMG_global algo





Lire pb1
{
Solide
{
        rho Champ_Uniforme 1 1000.
        lambda Champ_Uniforme 1 250.    
	Cp Champ_Uniforme 1 100.
}
        Conduction
        {
                diffusion { }
                conditions_initiales {
                        temperature Champ_Uniforme 1 30.
                        }
                boundary_conditions {
                        Gauche1 paroi_temperature_imposee
                                Champ_Front_Uniforme 1 40.
                        Haut1 paroi_temperature_imposee
                                Champ_Front_Uniforme 1 20.
                        Bas1 paroi_temperature_imposee
                                Champ_Front_Uniforme 1 40.
                        Bas2 paroi_temperature_imposee
                                Champ_Front_Uniforme 1 40.
                        Droit2 paroi_temperature_imposee
                                Champ_Front_Uniforme 1 20.
                        Droit1     Paroi_Echange_contact_VDF_Zoom_grossier
	       				T_ext Champ_front_zoom pbMG_global pb1 pb2 Droit1f temperature  
              				H_imp Champ_Front_Uniforme 1 1.e10								
                        Haut2     Paroi_Echange_contact_VDF_Zoom_grossier
	       				T_ext Champ_front_zoom pbMG_global pb1 pb2 Haut2f temperature  
              				H_imp Champ_Front_Uniforme 1 1.e10								
                }
        }
        Postraitement
        {
	 format lml
                Sondes
                {
                        sonde_tsol temperature periode 1. points 2 
                                0.15 0.55 
                                0.55 0.15 
                        coupe_tsol temperature periode 100. segment 3 0.05 0.6 0.25 0.6
                }
                Champs dt_post 100.
                {
                        temperature elem
                }
        }
        sauvegarde formatte solide.rep
}

Lire pb2
{
Fluide_Incompressible
{
	mu Champ_Uniforme 1 0.002
	rho Champ_Uniforme 1 2
        lambda Champ_Uniforme 1 1.0 
	Cp Champ_Uniforme 1 500
        beta_th Champ_Uniforme 1 0.0001
        gravite Champ_Uniforme 2 0 -9.81
}

        Navier_Stokes_standard
        {
                solveur_pression GCP { 
                        precond ssor { omega 1.500000 } 
                        seuil 1.000000e-14 
                }
                convection { amont }
                diffusion { }
                sources { boussinesq_temperature { T0 30. } }
                conditions_initiales {
                        vitesse Champ_Uniforme 2 0. 0.
                        }
                boundary_conditions {
                        Entree frontiere_ouverte_vitesse_imposee
                                Champ_front_Uniforme 2 0. -0.01
                        Sortie frontiere_ouverte_vitesse_imposee
                                Champ_front_Uniforme 2 0.01 0.
                        Droit1f paroi_fixe
                        Haut2f paroi_fixe
                        Haut3 paroi_fixe
                        Droit2 paroi_fixe
                }
        }
        Convection_Diffusion_Temperature
        {
                diffusion { }
                convection { amont }
                boundary_conditions 
                {
                        Entree frontiere_ouverte_temperature_imposee
                                Champ_front_Uniforme 1 20.
                        Sortie frontiere_ouverte_temperature_imposee
                                Champ_front_Uniforme 1 20.
                        Haut3 paroi_temperature_imposee
                                Champ_front_Uniforme 1 20.
                        Droit2 paroi_temperature_imposee
                                Champ_front_Uniforme 1 20.
                        Droit1f     Paroi_Echange_contact_VDF_Zoom_fin
	       				T_ext Champ_front_zoom pbMG_global pb2 pb1 Droit1 temperature  
              				H_imp Champ_Front_Uniforme 1 1.e10								
                        Haut2f     Paroi_Echange_contact_VDF_Zoom_fin
	       				T_ext Champ_front_zoom pbMG_global pb2 pb1 Haut2 temperature  
              				H_imp Champ_Front_Uniforme 1 1.e10								
                }
                conditions_initiales { Temperature Champ_Uniforme 1 30. }
        }
        Postraitement
        {
	format lml
                Sondes
                {
                        sonde_pression pression periode 1. points 1
                                0.55 0.55
                        sonde_vitesse vitesse periode 1. points 1
                                0.55 0.55
                        sonde_tflu temperature periode 1. points 1
                                0.55 0.55
                        sonde_seg temperature periode 100. segment 14 0.325 0.6 0.975 0.6
                         sonde_seg_vit vitesse periode 100. segment 14 0.325 0.6 0.975 0.6
              }
                Champs dt_post 100.
                {
                        pression elem
                        pression som
                        vitesse som
                        temperature elem
                }
        }
        sauvegarde formatte fluide.rep
}
Solve pbMG_global
Fin
Fin
