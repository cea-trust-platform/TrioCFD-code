IJK_Splitting grid_splitting
Lire grid_splitting
{
  ijk_grid_geometry DOM
  nproc_i 1
  nproc_j 1  
  nproc_k 1
}

IJK_FT_double
{
  vol_bulle_monodisperse 5.23583e-10
  ijk_splitting grid_splitting
  ijk_splitting_ft_extension 10 # To be checked.. #
  timestep 1e-4
  time_scheme euler_explicit # si euler_explicit diminuer le facsec #
  type_velocity_convection_op Centre
  Boundary_Conditions {
    bctype_kmin perio
    bctype_kmax perio
  }
  timestep_facsec 0.5
  cfl 1
  fo  1
  oh  1
  dt_post 50
  dt_post_stats_bulles 50
  dt_post_stats_plans 50
  t_debut_statistiques 0.0

  champs_a_postraiter 6 COMPO_CONNEXE INDICATRICE VELOCITY PRESSURE COURBURE INTERFACES
  check_stop_file stop.txt
  dt_sauvegarde 50
  nb_pas_dt_max 1
  
