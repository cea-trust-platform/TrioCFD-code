IJK_Grid_Geometry grid_geom

Lire grid_geom
{
  nbelem_i 64
  nbelem_j 64
  nbelem_k 64
  uniform_domain_size_i 0.1
  uniform_domain_size_j 0.1
  uniform_domain_size_k 0.1
  perio_i
  perio_j
 
}

IJK_Splitting grid_splitting
Lire grid_splitting
{
  ijk_grid_geometry grid_geom
  nproc_i 1
  nproc_j 1
  nproc_k 1
}

IJK_FT_double
{
  ijk_splitting grid_splitting
  timestep 0.008
  timestep_facsec 0.4
  dt_post 20
  champs_a_postraiter 1 tous
  check_stop_file stop.txt
  dt_sauvegarde 1000
  nb_pas_dt_max 3000
  
  multigrid_solver {
    solver_precision mixed
    coarsen_operators 4
      Coarsen_Operator_Uniform {  }
      Coarsen_Operator_Uniform {  }
      Coarsen_Operator_Uniform {  } 	
      Coarsen_Operator_Uniform {  } 	
    ghost_size 1
    pre_smooth_steps 1 7
    smooth_steps 1 7
    relax_jacobi 1 0.7
    solveur_grossier GCP { seuil 1e-10  precond ssor { omega 1.5 } }
    seuil 1e-8
    nb_full_mg_steps 2 20 1
    
  }
  check_divergence
  vitesse_entree 0.01
   rho_liquide 1000
   mu_liquide 1e-3
  expression_vx_init 0.01
  expression_vy_init 0.
  expression_vz_init 0.
  
}
Fin
