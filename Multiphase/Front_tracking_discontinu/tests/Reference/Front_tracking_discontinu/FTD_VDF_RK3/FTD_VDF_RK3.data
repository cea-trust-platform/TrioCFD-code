 #
 deplacement des interfaces a vitesse imposee
 couplage fluide : vitesse imposee par les interfaces
#
# PARALLEL STOPS #
dimension 3
Probleme_FT_Disc_gen pb
Domaine DOM

# BEGIN MESH #
Mailler DOM
{
	Pave pave1
	{
		origine 0. 0. 0.
		longueurs 0.015 0.05 0.015
		nombre_de_noeuds 30 100 30
	}
	{
		bord paroi X = 0.    0. <= Y <= 0.05  0. <= Z <= 0.015
		bord paroi X = 0.015 0. <= Y <= 0.05  0. <= Z <= 0.015
		bord paroi Y = 0.    0. <= X <= 0.015 0. <= Z <= 0.015
		bord haut  Y = 0.05  0. <= X <= 0.015 0. <= Z <= 0.015
		bord paroi Z = 0.    0. <= X <= 0.015 0. <= Y <= 0.05
		bord paroi Z = 0.015 0. <= X <= 0.015 0. <= Y <= 0.05
	}
}
# END MESH #
# BEGIN PARTITION
Partition DOM
{
	Partitionneur tranche { tranches 2 1 1 }
	Larg_joint 2
	Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones DOM
END SCATTER #


VDF dis

RK3_FT  sch
Lire sch
{
	tinit 0.
	tmax 0.2
	dt_min 1.e-7
	dt_max 2.e-5
	dt_impr 10
	dt_sauv 100
	facsec 0.5
	seuil_statio -1
	nb_pas_dt_max 3
}

Fluide_Incompressible liquide
Lire liquide
{
	mu  Champ_Uniforme 1 0.0222
 	rho Champ_Uniforme 1 851.
}

Fluide_Incompressible vapeur
Lire vapeur
{
	mu  Champ_Uniforme 1 0.000282
 	rho Champ_Uniforme 1 2.
}

Fluide_Diphasique fluide
Lire fluide
{
	fluide0 liquide
	fluide1 vapeur
	sigma           Champ_Uniforme 1 0.0292
}
Constituant constituant
Lire constituant
{
	coefficient_diffusion Champ_Uniforme 2 1e-12 0.
}

Champ_Uniforme gravite
Lire gravite 3 0 -9.81 0
Associate fluide gravite

Navier_Stokes_FT_Disc        hydraulique
Transport_Interfaces_FT_Disc interf
Convection_Diffusion_Concentration    eq_concentration

Associate pb hydraulique
Associate pb interf
Associate pb eq_concentration
Associate pb DOM
Associate pb sch
Associate pb fluide
Associate pb constituant

Discretize pb dis

System "mkdir -p lata"

Lire pb
{
hydraulique
{
	modele_turbulence sous_maille_wale
        {
        Cw 0.
        turbulence_paroi negligeable
        }
	solveur_pression GCP { precond ssor { omega 1.5 } seuil 1.e-12 impr }
	convection           { centre }
	diffusion            { }
	conditions_initiales { vitesse champ_uniforme 3 0. 0. 0. }
	equation_interfaces_proprietes_fluide interf
	boundary_conditions 
	{
		haut   Sortie_libre_rho_variable champ_front_uniforme 1 0. 
		paroi  paroi_fixe	
	}
}



interf
{
	methode_transport vitesse_interpolee hydraulique
	methode_interpolation_v vdf_lineaire
	conditions_initiales {
		fonction ((x*y*z)-((x-0.0075)^2+(y-0.0045)^2+(z-0.0075)^2-(0.001775)^2))
        }
	volume_impose_phase_1 2.3425e-8
	VOFlike_correction_volume 1
	nb_lissage_correction_volume 1
        nb_iterations_correction_volume 0
        n_iterations_distance 2
	remaillage { 
		pas 0.000001
		nb_iter_remaillage 1
		critere_arete 0.35 
		critere_remaillage 0.2 
		pas_lissage 0.0000001
		nb_iter_barycentrage 5
		relax_barycentrage 1 
		critere_longueur_fixe 0.000230
                lissage_courbure_coeff -0.05
		lissage_courbure_iterations 10
                nb_iter_correction_volume 0
                seuil_dvolume_residuel 1e-20
	}
	collisions {
		active
		juric_pour_tout
		type_remaillage Juric { source_isovaleur fonction_distance }
		   }
	boundary_conditions 
	{
		paroi Paroi_FT_disc symetrie
		haut  Paroi_FT_disc symetrie
	}	
}
eq_concentration
{
	convection { quick }
	diffusion  { }
	conditions_initiales
	{
	concentration Champ_fonc_xyz DOM 2 ((TANH(1000*y-11)+1.)*(TANH(17-900*y)+1.)/4) 0.
	}
	boundary_conditions
	{
	haut  frontiere_ouverte_concentration_imposee Champ_Front_Uniforme 2 0. 0.
	paroi paroi
	}
}

Postraitement
	{
		
		Champs dt_post 100
		{
			pression elem
			vitesse elem
			indicatrice_interf elem
		}
	}
liste_postraitements
{
	Postraitement_ft_lata post
	{	
		dt_post 0.005
		nom_fichier lata/post
		format binaire
		print
                champs elements
		{
			indicatrice_interf
			pression
			vitesse
		}
		champs sommets
		{
		}
                interfaces interf {
                        champs sommets {
                                pe
				courbure
                        }
                        champs elements {
                                pe
                        }
                }
	}
	Postraitement_ft_lata post2
	{	
		dt_post 0.005
		nom_fichier lata/post2
		format binaire
		print
                champs elements
		{
		}
		champs sommets
		{
		}
                interfaces interf {
                        champs sommets {
				courbure
                        }
                        champs elements {
                        }
                }
	}
}
}

Solve pb
Fin
