# Cas test Front-tracking discontinu VDF 2D #
# Test des forces de trainee et flottabilite #
# Test de l injection et de la condition de rebond sur les parois #
# PARALLEL OK #

dimension 2

Probleme_FT_Disc_gen pb
Domaine dom

# BEGIN MESH #

Mailler dom
{
	Pave Cavite 
		{
		Origine 0. 0.
     		Nombre_de_Noeuds 11 51
		Longueurs 0.2 1.
		}
		{
		Bord Gauche X = 0.  0. <= Y <= 1.
		Bord Haut Y = 1.    0. <= X <= 0.2
		Bord Bas Y = 0.     0. <= X <= 0.2
		Bord Droit X = 0.2  0. <= Y <= 1.
		}
}

Sous_Zone zone1
Associate zone1 dom
Lire zone1 {
   Rectangle
     Origine 0.05 0.1
     Cotes 0.1 0.1
}

Sous_Zone zone2
Associate zone2 dom
Lire zone2 {
   Rectangle
     Origine 0.05 0.4
     Cotes 0.1 0.1
}
# END MESH #

# BEGIN PARTITION
Partition dom
{
	Partitionneur tranche { tranches 2 1 }
	Larg_joint 2
	Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones dom
Sous_Zone zone1
Associate zone1 dom
Lire zone1 { Fichier zone1.ssz }
Sous_Zone zone2
Associate zone2 dom
Lire zone2 { Fichier zone2.ssz }
END SCATTER #

# Je choisis une discretisation #
VDF ma_discretisation

Schema_Euler_explicite mon_schema
Lire mon_schema
{
	tinit 0.
	tmax 1.
	dt_min 5.e-3
	dt_max 5.e-3
	dt_impr 5.e-3
	dt_sauv 100
	seuil_statio 1.e-8
	nb_pas_dt_max 200
}


Fluide_Incompressible liquide
Lire liquide
{
	mu  Champ_Uniforme 1 3.7e-05
 	rho Champ_Uniforme 1 2
 	 gravite Champ_Uniforme 2 0. -9.81

}

Navier_Stokes_FT_Disc          hydraulique
Transport_Marqueur_FT	       marqueurs

Associate pb hydraulique
Associate pb marqueurs

Associate pb dom
Associate pb mon_schema
Associate pb liquide

Discretize pb ma_discretisation

Lire pb
{
	hydraulique
	{
		modele_turbulence sous_maille_wale
        	{
           	  Cw 0.
          	  turbulence_paroi negligeable
        	}
		
		solveur_pression GCP { 
			precond ssor { omega 1.500000 } 
			seuil 1.000000e-6 
			impr 
		}
		convection { quick }
		diffusion { }
		conditions_initiales {
			vitesse Champ_Uniforme 2 0. 0.
			}
		
		boundary_conditions {
			Gauche paroi_fixe
			Droit paroi_fixe
			Bas  frontiere_ouverte_vitesse_imposee Champ_front_Uniforme 2 0. 1.
			Haut Sortie_libre_rho_variable champ_front_uniforme 1 0. 
		}
	        terme_gravite rho_g
	}
	
        
	marqueurs
	{
	 conditions_initiales {
	        ensemble_points { sous_zones 1 zone1 aleatoire 10 } 
		proprietes_particules { 
					fichier proprietes
		      		      }
		t_debut_integration 0.1
	 }
	 boundary_conditions
		{
		  
		}
		
         injection {
	        ensemble_points { sous_zones 1 zone2 aleatoire 2 }
	        proprietes_particules { 
					distribution { 
					nb_particules 2
		       			vitesse 0. 0.
					temperature 273.
					masse_volumique  1.
					diametre 0.01 
					}
		      	       }
		t_debut_injection 0.2      
	        dt_injection 0.3     
	 }	
	 		
         sources { Trainee , flottabilite }	
	 methode_transport vitesse_particules	
	 methode_couplage one_way_coupling 
	 nb_iterations 1
	}
	
	Postraitement
	{
		Sondes
		{
			sonde_pression_point pression periode 0.01 points 1 0.1 0.5
			sonde_vit_point vitesse periode 0.01 points 1 0.1 0.5
			sonde_vit_coupe vitesse periode 0.01 segment 20 0.1 0. 0.1 1.
			sonde_densite_point densite_particules periode 0.01 points 1 0.1 0.5
			sonde_densite_coupe densite_particules periode 0.01 segment 20 0.1 0. 0.1 1.
						
		}
        	Format lml
		Champs dt_post 0.1
		{
			pression elem
			pression som
			vitesse elem
			vitesse som
			densite_particules elem
			volume_particules elem
			
		}
		
	}
liste_postraitements
{	
        postraitement_ft_lata marqueurs { 
    format Lata 
    fichier marqueurs 
    champs dt_post 0.05 { 
        vitesse som 
    } 
    interfaces marqueurs { 
        vitesse som 
        volume som 
        diametre som 
        temperature som 
        masse_volumique som 
    } 
}	
}
}

Solve pb
Fin
 