#  
 Cas test Front-tracking discontinu VEF.
 Monophasique
 Interface IBC avec lecture MED
 PARALLEL OK 4
#

dimension 3
Probleme_FT_Disc_gen pb

Domaine DOM
# BEGIN MESH #
Read_file DOM cylindre_5000elts.geom
Reorienter_tetraedres DOM
Dilate DOM 0.0001
# END MESH #

# BEGIN PARTITION 
Partition DOM {
        partitionneur metis { Nb_parts 4 }
        larg_joint 2
        Nom_Zones DOM
}
Fin
END PARTITION #

# BEGIN SCATTER
Scatter DOM.Zones DOM
END SCATTER #

VEFPreP1B dis

Schema_Euler_explicite  sch
Lire sch
{
	tinit 0.0
	tmax  0.1
	dt_min 1.e-7
	dt_max 0.02
	dt_impr 10.
	dt_sauv 100
	seuil_statio -1
	nb_pas_dt_max 10
}

Fluide_Incompressible liquide
Lire liquide
{
	mu  Champ_Uniforme 1 0.282e-3
 	rho Champ_Uniforme 1 1000.
}

Navier_Stokes_FT_Disc              hydraulique
Transport_Interfaces_FT_Disc       body

Associate pb hydraulique
Associate pb body

Associate pb DOM
Associate pb sch
Associate pb liquide

Discretize pb dis

Lire pb
{
hydraulique
{
	modele_turbulence sous_maille_wale
        {
           Cw               1.e-16
           turbulence_paroi negligeable
        }

	solveur_pression GCP { precond ssor { omega 1.6 } seuil 1.e-12 impr }
	convection           { amont }
	diffusion            { }
	conditions_initiales { vitesse champ_uniforme 3 0. 0. 0. }
	equations_interfaces_vitesse_imposee 1   body
	boundary_conditions
	{
		haut   Sortie_libre_rho_variable champ_front_uniforme 1 0.
		paroi  paroi_fixe
		bas    paroi_fixe
	}
}
body
{
	conditions_initiales {
                fichier_geom {
                        fichier_geom halteres.geom
                        point_phase 0 0. 0. 0.1
                        default_phase 1
			reverse_normal
                        lata_dump connex
                }		
	}
	remaillage {
		pas -1e8
		pas_lissage -1
	}
	boundary_conditions
	{
		haut  Paroi_FT_disc symetrie
		paroi Paroi_FT_disc symetrie
		bas   Paroi_FT_disc symetrie
	}
	methode_transport vitesse_imposee -y*10. x*10. 0.
}
postraitement {
  champs dt_post 1. {
	vitesse elem
	indicatrice_body elem
  }
}
liste_postraitements {
   postraitement_ft_lata post1 {
	interfaces body { } 
	champs elements { indicatrice_body } 
	champs faces { vitesse }
   }
}
}

Solve pb
Fin
